// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int a, sqr,cub;
    printf("Enter the two numbers : ");
    scanf("%d",&a);
    sqr = a*a;
    cub = a*a*a;
    printf(" the sqr root of the number is %d\n",sqr);
    printf("The cub root of the number is %d",cub);
    return 0;
}
